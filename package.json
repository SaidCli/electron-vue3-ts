{
  "name": "electron-vue3-ts",
  "version": "1.6.3",
  "description": "electron-vue3-ts",
  "main": "dist/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "format": "prettier --write .",
    "release": "standard-version",
    "release:major": "standard-version --release-as major",
    "release:minor": "standard-version --release-as minor",
    "prepare": "husky install",
    "electron:generate-icons": "electron-icon-builder --input=./public/logo.png --output=build-icons --flatten"
  },
  "keywords": [
    "Vue3",
    "ts",
    "Electron",
    "template",
    "base"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SaidBaseTemplate/electron-vue3-ts.git"
  },
  "author": "guizimo",
  "license": "MIT",
  "engines": {
    "node": ">=20.19.0 || >=22.12.0"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "element-plus": "^2.10.6",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@electron/remote": "^2.1.3",
    "@types/node": "^24.2.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12",
    "electron-icon-builder": "^2.0.1",
    "electron-log": "^5.4.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.4",
    "prettier": "^3.6.2",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "unocss": "^66.4.2",
    "vite": "^7.1.1",
    "vite-electron-plugin": "^0.8.3",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-svg-loader": "^5.1.0"
  },
  "lint-staged": {
    "**/*.{js,ts,json}": "prettier --write ."
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
